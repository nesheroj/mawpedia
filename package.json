{
  "name": "mawpedia",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "url": "nesukun/mawpedia"
  },
  "bugs": {
    "url": "https://github.com/nesukun/mawpedia/issues"
  },
  "scripts": {
    "lint": "prettier --list-different '**/*.{js,json,scss}' && eslint .",
    "prettify": "prettier --write '**/*.{js,json,scss}'",
    "precommit": "lint-staged",
    "prepush": "npm run lint && npm test",
    "build": "npm run clean && if [ \"$NODE_ENV\" = \"production\" ]; then npm run build:prod; else npm run build:dev; fi",
    "build:dev": "NODE_ENV=development webpack",
    "build:prod": "NODE_ENV=production npm run transpile-server && webpack",
    "clean": "rm -rf dist",
    "transpile-server": "BABEL_ENV=node babel ./src -d ./dist --copy-files --ignore ./src/client",
    "migrate": "node scripts/migration.js",
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": "NODE_ENV=development npm run build && npm run watch",
    "start:prod": "NODE_ENV=production npm run migrate && node ./dist/index.jh",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "BABEL_ENV=node babel-watch ./src/index.js"
  },
  "author": "",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "^2.4.10",
    "@angular/http": "^2.4.10",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/router": "^3.4.10",
    "angulartics2": "^1.6.4",
    "autoprefixer": "^8.6.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "base-64": "^0.1.0",
    "config": "^1.30.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ioredis": "^3.2.2",
    "joi": "^13.4.0",
    "js-yaml": "^3.12.0",
    "json-loader": "^0.5.7",
