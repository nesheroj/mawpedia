    "koa-compress": "^3.0.0",
    "koa-favicon": "^2.0.1",
    "koa-helmet": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa-static": "^4.0.3",
    "ng2-translate": "^5.0.0",
    "node-sass": "^4.9.0",
    "notp": "^2.0.3",
    "postcss-loader": "^2.1.5",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.12",
    "remarkable": "^1.7.1",
    "rxjs": "^5.1.0",
    "sass-loader": "^7.0.3",
    "stats-webpack-plugin": "^0.6.2",
    "string-replace-loader": "^2.1.1",
