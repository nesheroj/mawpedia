    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.22.2",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "husky": "^0.14.3",
    "jsdom": "^11.11.0",
    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-fetch": "^2.1.2",
    "prettier": "^1.13.5",
    "supertest": "^3.1.0",
    "thirty-two": "^1.0.2",
    "webpack-cli": "^3.0.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --list-different",
      "eslint"
    ],
    "*.scss": [
      "prettier --list-different"
    ],
    "*.json": [
      "prettier --list-different"
    ]
  }
}
